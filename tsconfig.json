{
  "exclude": [".env"],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "resolveJsonModule": true,
  }
}